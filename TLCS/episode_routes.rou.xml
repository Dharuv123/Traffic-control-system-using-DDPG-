          begin="0" end="3600" vehsPerHour="1200"/>

    <flow id="flow_E_W" type="car" route="E_W"
          begin="0" end="3600" vehsPerHour="1200"/>

    <flow id="flow_N_S" type="car" route="N_S"
          begin="0" end="3600" vehsPerHour="900"/>

    <flow id="flow_S_N" type="car" route="S_N"
          begin="0" end="3600" vehsPerHour="900"/>

</routes>
